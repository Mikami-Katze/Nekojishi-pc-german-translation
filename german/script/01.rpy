# game/script/01.rpy:1289
translate german start_b6070db7:

    # avt "\"Alright... so do you need exactly?\" "
    avt "\"Also schön... {w=0.5}was genau möchtest du denn?\" "

# game/script/01.rpy:1291
translate german start_ffe3c9ed:

    # voice voice_file.lpdcat(1,20)
    # lpdcat difficult "\"I'm actually a guardian tiger of a small temple in Miaoli... \" "
    voice voice_file.lpdcat(1,20)
    lpdcat difficult "\"Ich bin genau genommen der Wächter Tiger eines kleinen Tempels in Miaoli... \" "

# game/script/01.rpy:1297
translate german start_2d048635:

    # voice voice_file.tig(1,34)
    # tig lift "\"You a guardian tiger? Impossible.\" "
    voice voice_file.tig(1,34)
    tig lift "\"Du bist ein Wächter Tiger? Unmöglich.\" "

# game/script/01.rpy:1301
translate german start_1bed3a6d:

    # voice voice_file.lpdcat(1,21)
    # lpdcat serious "\"...Whatever. I know it's weird, you don't need to tell me.\" "
    voice voice_file.lpdcat(1,21)
    lpdcat difficult "\"...wie auch immer. Ich weiß das es eigenartig ist, das musst du mir nicht sagen.\" "

# game/script/01.rpy:1305
translate german start_bf2c4258:

    # voice voice_file.lpdcat(1,22)
    # lpdcat serious "\"It may be rare to have a leopard cat as a guardian tiger, but I am. If you still question me, go ask my {font=tl/None/NotoSansCJKtc-Bold.otf}Fudegong{/font} and not me.\" "
    # lpdcat serious "\"After all, it's not like I signed up for this...\" "
    voice voice_file.lpdcat(1,22)
    lpdcat serious "\"Es mag selten sein eine Leopardenkatze als Wächter Tiger zu haben, aber das bin ich. Wenn du mir trozdem nicht glaubst, geh und frag meinen {font=tl/None/NotoSansCJKtc-Bold.otf}Fudegong{/font} und nicht mich.\" "
    lpdcat serious "\"Es ist nicht so das ich mich darum beworben hätte...\" "

# game/script/01.rpy:1307
translate german start_d57eb73d:

    # voice voice_file.lpdcat(1,23)
    # lpdcat serious "\"Going back to the point, our temple is now being abandoned.\" "
    # lpdcat serious "\"If no one takes care of my temple, my Fudegong will—\" "
    # lpdcat serious "\"By then, I will be forced out and have to live like a stray cat, and I don't want that to happen.\" "
    voice voice_file.lpdcat(1,23)
    lpdcat serious "\"Zurück zum Punkt, unser Tempel ist inzwischen verlassen.\" "
    lpdcat serious "\"Wenn niemand sich um meinen Tempel kümmert dann wird mein Fudegong—\" "
    lpdcat serious "\"An diesem Punkt werde ich gezwungen sein das leben einer streunenden Katze zu führen und ich möchte nicht, dass das passiert.\" "

# game/script/01.rpy:1309
translate german start_22966d3e:

    # voice voice_file.lpdcat(1,24)
    # lpdcat serious "\"So Liao, please, help me restore the temple, please?\" "
    # lpdcat serious "\"If you want any compensation, I'll do whatever you want...\" "
    voice voice_file.lpdcat(1,24)
    lpdcat serious "\"Also Liao, bitte, hilfst du mir meinen Tempel zu erneuern, bitte?\" "
    lpdcat serious "\"Wenn du etwas im Gegenzug möchtest, dann mach ich alles was du möchtest...\" "

# game/script/01.rpy:1311
translate german start_c5453f50:

    # avt  "\"Wait, {i}stray cat?{/i} \ I thought you weren't a ca—\" "
    avt "\"Moment, {i}streunende Katze?{/i} \ Ich dachte du wärst keine Ka—\" "

# game/script/01.rpy:1313
translate german start_b4f13721:

    # voice voice_file.lpdcat(1,25)
    # lpdcat angry "\"That's not the point here!\" " with vpunch
    voice voice_file.lpdcat(1,25)
    lpdcat angry "\"Das ist doch hier nicht der Punkt!\" " with vpunch

# game/script/01.rpy:1319
translate german start_96cfd651:

    # voice voice_file.tig(1,35)
    # tig normal "\"You can't have him. Liao lives under my care. There's no reason for him to serve in any other temple.\" "
    # tig normal "\"If he must, he may help you, but Liao still has to return home.\" "
    voice voice_file.tig(1,35)
    tig normal "\"Du kannst ihn nicht haben. Liao lebt unter meiner Fürsorge. Es gibt keinen Grund für ihn in einem anderen Tempel zu dienen.\" "
    tig normal "\"Wenn er muss, kann er dir helfen, aber Liao muss immernoch nach Hause zurückkehren.\" "

# game/script/01.rpy:1321
translate german start_10ae1cc2:

    # avt "\"Wait, when did I ever decide to go home?\" "
    avt "\Warte, wann hab ich entschieden wieder nach Hause zu gehen?\" "

# game/script/01.rpy:1325
translate german start_3e0c7cf7:

    # voice voice_file.lpdcat(1,26)
    # lpdcat difficult "\"If you can help me...\" "
    # lpdcat difficult "\"Liao, please, can you lend me your hand? I beg you.\" "
    voice voice_file.lpdcat(1,26)
    lpdcat difficult "\"Wenn du mir helfen kannst...\" "
    lpdcat difficult "\"Liao, bitte, {w=0.5}kannst du mir helfen? Ich flehe dich an.\" "

# game/script/01.rpy:1327
translate german start_1f16b332:

    # voice voice_file.lpdcat(1,27)
    # lpdcat difficult "\"I've gone through a lot to find you, and perhaps...\" "
    voice voice_file.lpdcat(1,27)
    lpdcat difficult "\"Ich musste eine Menge überstehen um dich zu finden, und vielleicht...\" "

# game/script/01.rpy:1329
translate german start_45ee8e63:

    # voice voice_file.lpdcat(1,28)
    # lpdcat panic "\"For hundreds of years, nobody else with your ability will appear...\" "
    # lpdcat panic "\"Let alone in time to save my temple...\" "
    voice voice_file.lpdcat(1,28)
    lpdcat panic "\"wird für hunder JAhre niemand mehr mit deiner Fähigkeit auftauchen...\" "
    lpdcat panic "\"Schon gar nicht in der Zeit in der mein Tempel gerettet werden muss...\" "

# game/script/01.rpy:1331
translate german start_6b1f19de:

    # avt "\"So, your temple is in Miaoli, right? Well as great as it sounds, reviving a temple is easier said than done. I—\" "
